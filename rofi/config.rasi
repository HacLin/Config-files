configuration {
 font:"JetBrains Mono NL SemiBold 16";
 show-icons:true;
 icon-theme:"Papirus";
 display-drun: "RUN";
 drun-display-format:"{name}";
 sort:true;
 disable-history: false;
 sidebar-mode : false;
 modi: "drun,window,emoji:/bin/rofimoji";
 combi-modi: "drun,window";
 terminal :"xfce4-terminal";
 run-shell-command: "{terminal} -e {cmd}";
 }
 @theme "themes/mytheme3" 



